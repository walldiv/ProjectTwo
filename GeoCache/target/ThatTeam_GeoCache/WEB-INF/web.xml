<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>readandwrite</servlet-name>
    <servlet-class>com.ex.servlets.GeoCacheReadAndWriteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>readandwrite</servlet-name>
    <url-pattern>/readandwrite</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>placeitem</servlet-name>
    <servlet-class>com.ex.servlets.GeoCachePlaceItem</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>placeitem</servlet-name>
    <url-pattern>/placeitem</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>removeitem</servlet-name>
    <servlet-class>com.ex.servlets.GeoCacheRemoveItem</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>removeitem</servlet-name>
    <url-pattern>/removeitem</url-pattern>
  </servlet-mapping>

</web-app>
